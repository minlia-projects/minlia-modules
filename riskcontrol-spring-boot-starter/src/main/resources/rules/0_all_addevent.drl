package rules;

import com.minlia.module.riskcontrol.event.Event
import com.minlia.module.riskcontrol.service.RiskEventListService

global RiskEventListService riskEventListService

rule "0_all_addevent"
    salience 0
    lock-on-active true
    when
        event:Event()
    then
        riskEventListService.createEvent(event);
end
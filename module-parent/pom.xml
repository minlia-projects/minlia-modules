            </dependency>

            <dependency>
                <groupId>com.minlia.cloud.modules</groupId>
                <artifactId>module-rebecca</artifactId>
                <version>${minlia.module.version}</version>
            </dependency>

            <dependency>
                <groupId>com.minlia.cloud.modules</groupId>
                <artifactId>module-attachment</artifactId>
                <version>${minlia.module.version}</version>
            </dependency>


